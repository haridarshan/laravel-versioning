{
  "name": "haridarshan/laravel-modules-composer-deps",
  "description": "Manage module specific composer dependencies for api modules in Laravel using nWidart/laravel-modules",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Hari Darshan Gorana",
      "email": "er.haridarshan@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "nwidart/laravel-modules": "^9.0 || ^10.0 || ^11.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.6",
    "laravel/framework": "^9.0 || ^10.0 || ^11.0"
  },
  "autoload": {
    "psr-4": {
      "Haridarshan\\Laravel\\NwidartModules\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Haridarshan\\Laravel\\NwidartModules\\NwidartModulesServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
